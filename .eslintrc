{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "root": true,
    "rules": {
        "camelcase": 1,
        "space-infix-ops": 1,
        "no-unused-vars": 1,
        "no-console": "off"
    },
    "env": {
        "browser": true,
        "amd": true,
        "jquery": true
    }
}